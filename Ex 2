string1 = input("Enter first string and one digit number separated with space: ")
string2 = input("Enter second string and one digit number separated with space: ")
count1 = 0
count2 = 0

for i in string1:
      count1 = count1 + 1
for j in string2:
      count2 = count2 + 1

# Print the larger string of them
if (count1 < count2):
      print ("Larger string is:")
      print (string2)
#elif (count1 == count2):
      print ("Both strings are equal.")
else:
      print ("Larger string is:")
      print (string1)
      
a = set(string1)
b = set (string2)
#print (a)
#print (b)

common = a & b

print ("If there are any common letters or numbers in both strings, they will be printed below:")
print (common)


number = [int(s) for s in string1.split() if s.isdigit()]
print ("The number you intered in the first string is:")
print (number)
